<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QC_Floor_Wall_Yes_Require_Day_Count</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( QC_Floor_Wall_Tile_Install__c  , &quot;Yes&quot;),
ISBLANK( Day_Count_Floor_Wall_Tile__c  ))</errorConditionFormula>
    <errorDisplayField>Day_Count_Floor_Wall_Tile__c</errorDisplayField>
    <errorMessage>When selecting QC Floor Wall Tile Install &quot;Yes&quot; the Day Count for Floor Wall Tile must be entered.</errorMessage>
</ValidationRule>
