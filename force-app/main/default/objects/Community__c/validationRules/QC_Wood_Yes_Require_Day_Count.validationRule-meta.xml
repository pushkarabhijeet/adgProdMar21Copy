<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QC_Wood_Yes_Require_Day_Count</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( QC_Wood_Install__c  , &quot;Yes&quot;),
ISBLANK( Day_Count_Wood__c ))</errorConditionFormula>
    <errorDisplayField>Day_Count_Wood__c</errorDisplayField>
    <errorMessage>When selecting QC Wood Install &quot;Yes&quot; the Day Count for Wood must be entered.</errorMessage>
</ValidationRule>
