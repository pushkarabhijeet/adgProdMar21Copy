    <rules>
        <fullName>Case Origin set to QC</fullName>
        <actions>
            <name>Case_Origin_to_QC</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Case.Origin</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.Owner_is_Queue__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Case.RecordTypeId</field>
            <operation>equals</operation>
            <value>Standard</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Case%3A Images email to case default fields</fullName>
        <actions>
