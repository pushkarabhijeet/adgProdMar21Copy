            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Case.Owner_is_Queue__c</field>
            <operation>equals</operation>
            <value>Images Warranty/Occupied Queue</value>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Case%3A Remove Fire Assignment Check</fullName>
        <actions>
            <name>Case_Remove_Fire_Assignment_Check</name>
